{
  "analysis_years": [
    2019,
    2020
  ],
  "directories": {
    "log_directory": "./logs/",
    "output_directory": "./"
  },
  "execution_control": {
    "nodes": 10,
    "option": "slurm",
    "sites_per_worker": 20,
    "max_workers": 1,
    "sh_script": "source ~/start_hsds.sh"
  },
  "log_level": "DEBUG",
  "output_request": [
    "cf_mean",
    "cf_profile",
    "lcoe_fcr",
    "ghi_mean"
  ],
  "project_points": "./nsrdb_points_front_range.csv",
  "resource_file": "/nrel/nsrdb/v3/nsrdb_{}.h5",
  "sam_files": {
    "def": "./pv.json"
  },
  "technology": "pvwattsv7"
}
